{
  "name": "dunscombe-family-movies",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "db:init": "node ./fauna/init.js",
    "db:init:override": "node ./fauna/init.js --override",
    "db:gen": "dotenv -e .env.local graphql-codegen --config codegen.yml",
    "dev": "next dev",
    "build": "next build",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write **/*.{js,ts,tsx}",
    "lint": "eslint --fix 'src/**/*.{js,jsx,ts,tsx}'",
    "lint-staged": "lint-staged",
    "start": "next start",
    "test": "jest",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run type-check"
    }
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "npm run lint",
      "npm run format"
    ]
  },
  "dependencies": {
    "@hapi/iron": "^6.0.0",
    "@types/lodash": "^4.14.168",
    "axios": "^0.21.1",
    "cookie": "^0.4.1",
    "faunadb": "^4.1.3",
    "graphql": "^15.5.0",
    "lodash": "^4.17.21",
    "next": "10.0.8",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-query": "^3.13.0",
    "video.js": "^7.11.4"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.10.0",
    "@graphql-codegen/add": "^2.0.2",
    "@graphql-codegen/cli": "1.21.3",
    "@graphql-codegen/introspection": "1.18.1",
    "@graphql-codegen/typescript": "1.21.1",
    "@graphql-codegen/typescript-operations": "1.17.15",
    "@graphql-codegen/typescript-react-query": "^1.2.4",
    "@tailwindcss/custom-forms": "^0.2.1",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.31",
    "@types/react": "^17.0.2",
    "@types/video.js": "^7.3.15",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "autoprefixer": "^10.2.5",
    "dotenv": "^8.2.0",
    "dotenv-cli": "^4.0.0",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "husky": "^5.1.3",
    "jest": "^26.6.3",
    "jest-watch-typeahead": "^0.6.1",
    "lint-staged": "^10.5.4",
    "postcss": "^8.2.8",
    "prettier": "^2.2.1",
    "tailwindcss": "^2.0.3",
    "typescript": "^4.2.3"
  }
}
